<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>gestion</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginS</servlet-name>
    <servlet-class>controle.LoginS</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ajouterSEANCE</servlet-name>
    <servlet-class>controle.ajouterSEANCE</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AjoutMod</servlet-name>
    <servlet-class>controle.AjoutMod</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ajouterENS</servlet-name>
    <servlet-class>controle.ajouterENS</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SajouterETU</servlet-name>
    <servlet-class>controle.SajouterETU</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginS</servlet-name>
    <url-pattern>/LoginS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ajouterSEANCE</servlet-name>
    <url-pattern>/ajouterSEANCE</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AjoutMod</servlet-name>
    <url-pattern>/AjoutMod</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ajouterENS</servlet-name>
    <url-pattern>/ajouterENS</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SajouterETU</servlet-name>
    <url-pattern>/SajouterETU</url-pattern>
  </servlet-mapping>
</web-app>